--- a/arch/arm64/boot/dts/renesas/r8a779a0-falcon-cpu.dtsi
+++ b/arch/arm64/boot/dts/renesas/r8a779a0-falcon-cpu.dtsi
@@ -164,6 +164,7 @@ &mmc0 {
 	cd-gpios = <&gpio0 16 GPIO_ACTIVE_LOW>;
 	wp-gpios = <&gpio0 15 GPIO_ACTIVE_HIGH>;
 	bus-width = <4>;
+	disable-wp;
 	status = "okay";
 };
 #endif
